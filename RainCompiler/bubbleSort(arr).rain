assign nterms = input_int("How many terms? ")
assign n1=0 
assign n2 =  1
assign count = 0
if nterms <= 0 then
   print("Please enter a positive integer")
elif nterms == 1 then
   print("Fibonacci sequence upto")
   print(n1)
else
   print("Fibonacci sequence:")
   while count < nterms then
       print(n1)
       assign nth = n1 + n2
       
       n1 = n2
       n2 = nth
       count = count + 1
    end
end
pause()